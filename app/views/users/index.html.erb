<% provide(:title, "Phraeeをご利用中のメンバー一覧") %>
<% provide(:description, "Phraeeをご利用中のメンバー一覧") %>

  <%# サイトメインタイトル %>
  <%= render 'shared/main-title' %>

  <!-- パンくずリスト -->
  <ol class="breadcrumb" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">
    <!-- トップページ -->
    <li itemscope itemtype="http://data-vocabulary.org/Breadcrumb" class="home">
      <%= link_to root_path, itemprop:"url" do %>
      <span itemprop="title">トップ</span>
      <% end %>
      <span class="divider">/</span>
    </li>
    <!-- 現在ページ -->
    <li class="active" itemprop="child" itemscope itemtype="http://data-vocabulary.org/Breadcrumb">
      <span itemprop="title">メンバー</span>
    </li>
  </ol>

  <!-- page header -->
  <div class="page-header">
    <h2><span class="ph_category_name">Phraeeをご利用中のメンバー一覧</span></h2>
  </div>

  <%= render 'shared/members', object: @users %>

  <%= render 'shared/pagination', object: @users %>

  <% unless signed_in? %>
    <%= render 'shared/start-phrasee_link' %>
  <% end %>

  <p>&nbsp;</p>
  <%= render 'shared/post_phrase_link' %>
  <%= render 'shared/top-page_link' %>