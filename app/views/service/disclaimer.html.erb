<% provide(:title, "免責事項") %>
<% provide(:description, "免責事項") %>

<div class="container">

  <div class="row-fluid">
    <%# サイトメインタイトル %>
    <%= render 'shared/main-title' %>
  </div>
  
  <!-- パンくずリスト -->
  <ol class="breadcrumb">
    <li>
      <%= link_to 'トップ', root_path %>
      <span class="divider">/</span>
    </li>
    <li class="active">免責事項</li>
  </ol>

  <!-- page header -->
  <div class="page-header">
    <h3><%= image_tag('characters/t9elephant_trans.png', { alt:"elephant-icon", width:"55" } ) %>免責事項</h3>
  </div>

  <div class="container">
    <% @lines.each do |line| %>
    <dl>
      <dt><%= line.first %></dt>
      <dd><%= line.last %></dd>
    </dl>
    <% end %>
  </div>

  <p>&nbsp;</p>
  <%= render 'shared/top-page_link' %>
</div>